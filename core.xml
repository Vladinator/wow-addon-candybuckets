<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Frame name="CandyBucketsPinTemplate" inherits="BaseHighlightableMapPoiPinTemplate" enableMouseClicks="true" mixin="CandyBucketsPinMixin" virtual="true">
		<Layers>
			<Layer level="BORDER">
				<Texture parentKey="Border">
					<Anchors>
						<Anchor point="TOPLEFT" x="-1" y="1" />
						<Anchor point="BOTTOMRIGHT" x="1" y="-1" />
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
	<Frame name="CandyBucketsStatsTemplate" inherits="BaseHighlightableMapPoiPinTemplate" enableMouseClicks="true" mixin="CandyBucketsStatsMixin" virtual="true">
		<Layers>
			<Layer level="BORDER">
				<FontString parentKey="Text" inherits="SystemFont_Shadow_Med2" setAllPoints="true" justifyH="CENTER" justifyV="MIDDLE">
					<Anchors>
						<Anchor point="TOPLEFT" x="-1" y="1" />
						<Anchor point="BOTTOMRIGHT" x="1" y="-1" />
					</Anchors>
					<Color r="1.0" g="0.82" b="0" />
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>